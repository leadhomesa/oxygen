{
  "name": "@leadhome/oxygen",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:es --copy-files",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "node scripts/copy-files.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leadhomesa/oxygen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leadhomesa/oxygen/issues"
  },
  "homepage": "https://github.com/leadhomesa/oxygen#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
